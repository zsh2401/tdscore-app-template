{
  "name": "tdscore-app-template",
  "version": "0.0.1",
  "main": "index.js",
  "author": "Seymour Zhang",
  "license": "MIT",
  "scripts": {
    "build:qjs": "cross-env TS_NODE_PROJECT=\"./tsconfig.webpack.json\" webpack --config ./webpack.config.qjs.ts",
    "build:qapp": "npm run build:qjs && qjsc dist/qapp.min.js -o dist/qapp",
    "build:es5": "cross-env TS_NODE_PROJECT=\"./tsconfig.webpack.json\" webpack --config ./webpack.config.es5.ts",
    "build": "npm run build:es5 && npm run build:qjs",
    "start": "ts-node src/index.ts"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "tdscore": "^0.1.31"
  },
  "devDependencies": {
    "@types/terser-webpack-plugin": "^5.0.3",
    "@types/webpack-bundle-analyzer": "^3.9.2",
    "cross-env": "^7.0.3",
    "process": "^0.11.10",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.6.0"
  }
}
